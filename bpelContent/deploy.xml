<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:booking="http://eclipse.org/bpel/sample/booking" xmlns:travel_ws="urn:travel_ws">
  <process name="booking:Booking">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="booking:BookingService" port="BookingPort"/>
    </provide>
    <invoke partnerLink="TravelPL">
      <service name="travel_ws:travel_service" port="travel_services_port"/>
    </invoke>
  </process>
</deploy>